<!-- map_server.launch -->
<launch >
    <arg name="map_name" default="/map" />
    <!-- <arg name="map_yaml_file" default="$(find robot_bringup)/map/b3-4.yaml" /> -->
    <arg name="map_yaml_file" default="$(find robot_bringup)/map/icra2019.yaml" />

    <node name="amcl_map_server" pkg="map_server" type="map_server" args="$(arg map_yaml_file)" >
        <remap from="/map" to="$(arg map_name)" />
    </node >
</launch >
